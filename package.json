    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.0",
    "@types/serve-favicon": "^2.5.2",
    "@types/validator": "^10.11.3",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "axios": "^0.21.0",
    "eslint": "^7.15.0",
    "mocha": "^8.2.1",
    "shx": "^0.3.3",
